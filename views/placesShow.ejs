<link href="/CSS/table.css" rel="stylesheet" />
<div class="para">Exhibition Available at this Places:</div>
<% var Length = places.length%>

    <table id="tb">
        <thead>
            <tr class="firstt">
                <th scope="col" class="firstt">Places</th>
            </tr>
        </thead>
        <% for(var i=0; i<Length; i++){ %>

            <tbody class="firstt">
                <tr class="firstt">
                    <td class="firstt">
                        <%= places[i].place %>
                    </td>
                </tr>
                <br>
            </tbody>
            <% } %>
    </table>